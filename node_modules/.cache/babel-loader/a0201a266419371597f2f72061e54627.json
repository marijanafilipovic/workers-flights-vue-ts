A,WAAA,GAAc,IAAA,CAAA,WAAd,GAAyB,KAAQ,OAAR,CAAe,CAAf,EAAmB,KAGuC;AAFhG,IAAA,QAAQ,EAAE,IAAA,CAAA,QAAA,GAAW,IAAA,CAAA,QAAX,GAAmB,KAAQ,OAAR,CAAe,CAAf,EAAmB,QAEgD;AADhG,IAAA,UAAU,EAAE,IAAA,CAAA,UAAA,GAAa,IAAA,CAAA,UAAb,GAAuB,KAAQ,OAAR,CAAe,CAAf,EAAmB,SAC0C;AAAhG,yBAAmB,IAAA,CAAA,eAAA,GAAkB,IAAA,CAAA,eAAlB,GAAiC,KAAQ,OAAR,CAAe,CAAf,EAAmB;AAAyB,GAHhH,E,IAAA,E;;AAAA,I,8DAAA,CAaU,EAPJ,mBAAA,CAMM,KANN,EAAA,WAAA,EAMM,CALJ,WAKI,EAJJ,mBAAA,CAAyE,GAAzE,EAAyE,IAAzE,EAAG,kBAAa,gBAAA,CAAE,IAAA,CAAA,WAAA,GAAc,IAAA,CAAA,WAAd,GAAyB,KAAQ,OAAR,CAAe,CAAf,EAAmB,KAA9C,CAAhB,EAAmE;AAAA;AAAnE,GAII,EAHJ,mBAAA,CAA+B,GAA/B,EAA+B,IAA/B,EAAG,cAAS,gBAAA,CAAG,IAAA,CAAA,QAAH,CAAZ,EAAuB;AAAA;AAAvB,GAGI,EAFJ,mBAAA,CAAmC,GAAnC,EAAmC,IAAnC,EAAG,iBAAY,gBAAA,CAAG,IAAA,CAAA,UAAH,CAAf,EAA4B;AAAA;AAA5B,GAEI,EADJ,mBAAA,CAA8C,GAA9C,EAA8C,IAA9C,EAAG,sBAAiB,gBAAA,CAAG,IAAA,CAAA,eAAH,CAApB,EAAsC;AAAA;AAAtC,GACI,CANN,CAOI,CArCN,CAsCI,CAvCN,CAyCI,CA5CN,C","sourcesContent":["<template>\n<div id=\"flightstable\">\n\n   <h1>Welcome to {{ appName }}!</h1>\n  <div class=\"row\" style=\"text-align: center; margin: 0 auto;\">\n    <div id=\"flightsWorkers\" class=\"col-md-12  \">\n      <div id=\"worker-list\" style=\"width:30%; float: left; \">\n        <h3>Workers</h3>\n        <div v-for=\"worker in workers\" :key=\"worker.id\">\n          <p @click=\"intervalFetch( worker.id );\" >{{ worker.name }}</p>\n        </div>\n    </div>\n    <div id=\"flight-list\" style=\"width:50%; float:left;\">\n      <h3>Flights</h3>\n      <div v-for=\"flight in flights\" :key=\"flight.num\" >\n        <p @click=\"fDetails(flight.plane, flight.duration, flight.from_gate, flight.to_gate)\" style=\"overflow: hidden\">\n          FLIGHT: {{ flight.num }}\n          FROM : {{ flight.from }} TO: {{ flight.to }}\n          FROM DATE:{{ flight.from_date }} TO DATE:{{ flight.to_date}}\n          PLANE: {{ flight.plane}} DURATION: {{ flight.duration }}\n          FROM GATE: {{ flight.from_gate}} TO GATE: {{ flight.to_gate}}\n          <!--        MAKE COMMPONET AND BIND FOR FLIGHT DETAILS\n                      MAKE FILTER FOR DURATION USE PIPE\n          -->\n        </p><br/>\n      </div>\n    </div>\n\n\n<FlightDetails :planeNumber=\"planeNumber ? planeNumber : this.flights[0].plane\"\n               :duration=\"duration ? duration : this.flights[0].duration\"\n               :originGate=\"originGate ? originGate : this.flights[0].from_gate\"\n               :destinatioin-gate=\"destinationGate ? destinationGate : this.flights[0].to_gate\"></FlightDetails>\n\n\n      <div id=\"flight-details\" style=\"width:20%; float:left;\">\n        <h3>FLIGHT DETAILS</h3>\n        <p>Plane Number {{planeNumber ? planeNumber : this.flights[0].plane}}</p>\n        <p>Duration {{ duration  }}</p>\n        <p>Origin gate {{ originGate }}</p>\n        <p>Destination gate {{ destinationGate  }}</p>\n      </div>\n    </div>\n  </div>\n\n</div>\n\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent } from 'vue';\n import axios    from \"axios\";\n\n  import Worker from \"@/type/Worker\";\n  import Flight from \"@/type/Flight\";\n  import FlightDetails from \"@/components/FlightDetails.vue\";\nexport default defineComponent({\n  name: 'App',\n  components: { FlightDetails },\n   data(){\n    return {\n      appName : 'WORKERS FLIGHTS',\n      flights:[] as Flight[],\n      workers:[] as Worker[],\n      firstID: [0],\n      intervals: [0],\n      planeNumber: '',\n      duration: '',\n      originGate: '',\n      destinationGate: ''\n\n    }},\n  mounted() {\n    //this.fDetails(this.flights[0].plane  , this.flights[0].duration, this.flights[0].from_gate, this.flights[0].to_gate );\n\n   this.getWorkerList();\n },\nasync created(){\n    await this.getWorkerList();\n        this.viewFlightList(this.getFirstID());\n},\n  methods: {\n    fDetails(planeNumber: string , flightDuration: any, originGate: any, destinationGate: any ){\n        this.planeNumber =  planeNumber;\n        this.duration = flightDuration.toString();\n        this.originGate = originGate;\n        this.destinationGate= destinationGate;\n        //return [this.planeNumber, this.duration, this.originGate, this.destinationGate]\n    },\n    getWorkerList(){\n    return  axios\n          .get<Worker[]>('https://interview-mock.herokuapp.com/api/workers/')\n          .then( response => (this.workers = response.data))\n    },\n    getFirstID(){\n      return this.workers[0].id;\n       //console.log(this.workers[0].id);\n    },\n    viewFlightList(workerID: number) {\n      //  window.clearInterval(this.interval);\n      axios\n          .get<Flight[]>('https://interview-mock.herokuapp.com/api/workers/' + workerID)\n          .then(response => (this.flights = response.data))\n\n     // this.fDetails(this.flights[0].plane  , this.flights[0].duration, this.flights[0].from_gate, this.flights[0].to_gate );\n    },\n    intervalFetch(workerID: number){\n      this.viewFlightList(workerID);\n\n      if(this.intervals.length > 0){\n        for(let i = 0, length = this.intervals.length; i < length; i++){\n          //console.log(key);\n          clearInterval(this.intervals[i]);\n        }\n        let interval =  setInterval(()=> {\n          this.viewFlightList(workerID);\n        }, 6000);\n        // console.log(interval);\n        this.intervals.push(interval)\n/*        this.planeNumber = this.flights[0].plane;\n        this.duration = this.flights[0].duration.toString();\n        this.originGate = this.flights[0].from_gate;\n        this.destinationGate = this.flights[0].to_gate;*/\n\n      }\n      if(this.intervals.length == 0) {\n        let interval =  setInterval(()=> {\n          this.viewFlightList(workerID);\n        }, 6000);\n        // console.log(interval);\n        this.intervals.push(interval)\n       /* this.planeNumber = this.flights[0].plane;\n        this.duration = this.flights[0].duration.toString();\n        this.originGate = this.flights[0].from_gate.toString();\n        this.destinationGate = this.flights[0].to_gate.toString();*/\n      }\n\n    },\n  }\n});\n</script>\n\n<style>\n\n#flightstable{\n  height: 100%;\n  overflow: hidden;\n  background-color: lightgray;\n}\n.active{\n  background-color: aliceblue;\n}\n</style>\n"],"sourceRoot":""}]}