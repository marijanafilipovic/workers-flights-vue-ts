  if(this.intervals.length > 0){\n        for(let i = 0, length = this.intervals.length; i < length; i++){\n          //console.log(key);\n          clearInterval(this.intervals[i]);\n        }\n        let interval =  setInterval(()=> {\n          this.viewFlightList(workerID);\n        }, 6000);\n        // console.log(interval);\n        this.intervals.push(interval)\n/*        this.planeNumber = this.flights[0].plane;\n        this.duration = this.flights[0].duration.toString();\n        this.originGate = this.flights[0].from_gate;\n        this.destinationGate = this.flights[0].to_gate;*/\n\n      }\n      if(this.intervals.length == 0) {\n        let interval =  setInterval(()=> {\n          this.viewFlightList(workerID);\n        }, 6000);\n        // console.log(interval);\n        this.intervals.push(interval)\n       /* this.planeNumber = this.flights[0].plane;\n        this.duration = this.flights[0].duration.toString();\n        this.originGate = this.flights[0].from_gate.toString();\n        this.destinationGate = this.flights[0].to_gate.toString();*/\n      }\n\n    },\n  }\n});\n"],"sourceRoot":""}]}